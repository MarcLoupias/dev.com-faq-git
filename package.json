{
    "name": "faq-git",
    "version": "0.1.0",
    "description": "FAQ Git pour developpez.com",
    "author": "marlou <pro@marc-loupias.fr>",
    "private": true,
    "license": "CC-BY-SA-4.0",
    "engines": {
        "node": ">=6.0.0",
        "npm": ">=5.2.0"
    },
    "devDependencies": {
        "dvlp-faq-md-summary": "^1.0.0",
        "dvlp-faq-xml": "^1.0.0",
        "husky": "^1.3.1",
        "markdownlint-cli": "^0.13.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm test"
        }
    },
    "scripts": {
        "test": "node node_modules/.bin/markdownlint faq-content/**/*.md",
        "build-summary": "node node_modules/.bin/mdfc convert 'dvlp-faq-md-summary' 'faq-content/**/*.md' --dest 'faq-content/' --filename 'SUMMARY'",
        "build-xml": "node node_modules/.bin/mdfc convert 'dvlp-faq-xml' 'faq-content/**/*.md' --dest 'xml/' --filename 'dvlp-faq-git'"
    }
}
